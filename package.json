{
  "name": "gitzza-plaza",
  "version": "0.0.1",
  "description": "A public repository for Brunchstory Book “Git drawing.”",
  "repository": {
    "type": "git",
    "url": "https://github.com/enchic/gitzza-plaza.git"
  },
  "author": "공인식 (Enchic Gong)",
  "devDependencies": {
    "@types/markdown-it-container": "^2.0.10",
    "mermaid": "^11.4.0",
    "puppeteer": "^22.13.1",
    "puppeteer-core": "^22.13.1",
    "vitepress": "^1.6.3",
    "vitepress-export-pdf": "^1.0.0",
    "vitepress-plugin-mermaid": "^2.0.17",
    "ws": "^8.17.1"
  },
  "scripts": {
    "docs:dev": "vitepress dev sample-docs",
    "docs:build": "vitepress build sample-docs",
    "docs:preview": "vitepress preview sample-docs",
    "export-pdf-cover": "press-export-pdf export sample-docs -c sample-docs/.vitepress/vitepress-pdf-cover.config.ts",
    "export-pdf": "press-export-pdf export sample-docs",
    "pdf 커버 생성": "npm run export-pdf-cover",
    "pdf 생성": "npm run export-pdf",
    "dummy:novel7chapters:dev": "vitepress dev dummy/novel-7chapters",
    "dummy:novel7chapters:build": "vitepress build dummy/novel-7chapters",
    "export-pdf:dummy:novel7chapters": "press-export-pdf export dummy/novel-7chapters"
  },
  "dependencies": {
    "markdown-it-anchor": "^9.0.1",
    "markdown-it-container": "^4.0.0",
    "markdown-it-front-matter": "^0.2.4",
    "pagedjs": "^0.4.3",
    "vue": "^3.4.27"
  }
}
